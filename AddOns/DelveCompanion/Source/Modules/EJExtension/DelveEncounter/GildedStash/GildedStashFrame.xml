<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/Gethe/wow-ui-source/live/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
    <Frame name="DelveCompanionDelveEncounterGildedStashFrameTemplate"
        mixin="DelveCompanion_DelveEncounterGildedStashFrameMixin" virtual="true">
        <Size x="100" y="100" />
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="RewardCardBG" atlas="ui-journeys-delve-reward-bar"
                    useAtlasSize="true" setAllPoints="true" />
            </Layer>
            <Layer level="OVERLAY">
                <FontString parentKey="ErrorLabel" inherits="GameFontDisableMed2"
                    justifyH="CENTER" text="ERROR" mixin="AutoScalingFontStringMixin"
                    wordwrap="true" maxLines="4" justifyV="MIDDLE" hidden="true">
                    <Size x="80" y="80" />
                    <Anchors>
                        <Anchor point="CENTER" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Frame parentKey="Widget" hidden="true">
                <Size x="80" y="80" />
                <Anchors>
                    <Anchor point="CENTER" />
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture parentKey="Icon">
                            <Size x="60" y="60" />
                            <Anchors>
                                <Anchor point="CENTER" />
                            </Anchors>
                        </Texture>
                    </Layer>
                    <Layer level="ARTWORK" textureSubLevel="-1">
                        <Texture parentKey="FadeBg"
                            file="Interface\Common\Common-Input-Border" hidden="true">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativeKey="$parent"
                                    relativePoint="TOPLEFT" />
                                <Anchor point="BOTTOMRIGHT" relativeKey="$parent"
                                    relativePoint="BOTTOMRIGHT" />
                            </Anchors>
                            <TexCoords left="0.0625" right="0.9375" top="0.25" bottom="0.375" />
                        </Texture>
                    </Layer>
                    <Layer level="ARTWORK" textureSubLevel="0">
                        <Texture parentKey="CheckMark"
                            atlas="common-icon-checkmark" hidden="true">
                            <Size x="16" y="16" />
                            <Anchors>
                                <Anchor point="BOTTOMRIGHT" />
                            </Anchors>
                        </Texture>
                        <Texture parentKey="RedX"
                            atlas="common-icon-redx" hidden="true">
                            <Size x="16" y="16" />
                            <Anchors>
                                <Anchor point="BOTTOMRIGHT" />
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
            </Frame>
        </Frames>
        <Scripts>
            <OnLoad method="OnLoad" />
            <OnShow method="OnShow" />
            <OnHide method="OnHide" />
        </Scripts>
    </Frame>
</Ui>